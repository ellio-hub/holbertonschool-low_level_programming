#include "holberton.h"
#include <stdio.h>
/**
 * print_to_98 - main function
 * @n: parameter
 * Return: n
 */
void print_to_98(int n)
{
int x;
if (n < 98)
{
for (x = n; x <= 98; x++)
if (x != 98)
printf("%x, ", x);
else
printf("%x\n", x);
}
else
{
for (x = n; x >= 98; x--)
if (x != 98)
printf("%x, ", x);
else
printf("%x\n", x);
}
}
